    "node-mpg123": "^1.0.2-23",
    "colors.js": "^1.2.4",
    "colors": "git+https://github.com/djulien/colors.js.git",
    "debug": "^2.2.0",
    "sprintf.js": "^0.1.4",
    "napi_thread_safe_promise": "^1.1.2",
    "readable-stream": "^2.0.5",
